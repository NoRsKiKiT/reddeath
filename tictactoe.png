<svg xmlns="http://www.w3.org/2000/svg" width="200" height="200" viewBox="0 0 200 200">
  <rect width="200" height="200" fill="#1a1a1a"/>
  
  <!-- Фон игрового поля -->
  <rect x="40" y="40" width="120" height="120" fill="#333333"/>
  
  <!-- Горизонтальные линии -->
  <line x1="40" y1="80" x2="160" y2="80" stroke="#ffffff" stroke-width="2"/>
  <line x1="40" y1="120" x2="160" y2="120" stroke="#ffffff" stroke-width="2"/>
  
  <!-- Вертикальные линии -->
  <line x1="80" y1="40" x2="80" y2="160" stroke="#ffffff" stroke-width="2"/>
  <line x1="120" y1="40" x2="120" y2="160" stroke="#ffffff" stroke-width="2"/>
  
  <!-- Крестики -->
  <line x1="50" y1="50" x2="70" y2="70" stroke="#4ade80" stroke-width="3"/>
  <line x1="70" y1="50" x2="50" y2="70" stroke="#4ade80" stroke-width="3"/>
  
  <line x1="130" y1="50" x2="150" y2="70" stroke="#4ade80" stroke-width="3"/>
  <line x1="150" y1="50" x2="130" y2="70" stroke="#4ade80" stroke-width="3"/>
  
  <line x1="50" y1="130" x2="70" y2="150" stroke="#4ade80" stroke-width="3"/>
  <line x1="70" y1="130" x2="50" y2="150" stroke="#4ade80" stroke-width="3"/>
  
  <!-- Нолики -->
  <circle cx="100" cy="60" r="15" stroke="#ef4444" stroke-width="3" fill="none"/>
  <circle cx="60" cy="100" r="15" stroke="#ef4444" stroke-width="3" fill="none"/>
  <circle cx="140" cy="100" r="15" stroke="#ef4444" stroke-width="3" fill="none"/>
  <circle cx="100" cy="140" r="15" stroke="#ef4444" stroke-width="3" fill="none"/>
</svg>